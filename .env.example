# Supabase Configuration
# Get these from your Supabase project dashboard: https://supabase.com/dashboard
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_KEY=your_supabase_anon_key

# Database URL (for Drizzle migrations and server-side)
# Local: postgresql://postgres:postgres@127.0.0.1:54328/postgres
# Remote: Get from Supabase Dashboard > Settings > Database
DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54328/postgres

# App URLs (for local development)
VITE_AUTH_BASE_URL=http://localhost:4200
VITE_HQ_BASE_URL=http://localhost:4201

# Branding
VITE_BRAND_NAME=Arcata
